<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:Wise.Framework.Presentation.View"
                    xmlns:viewModel="clr-namespace:Wise.Framework.Presentation.ViewModel">

    <DataTemplate DataType="{x:Type viewModel:ShellViewModel}">
        <view:ShellView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:StatusViewModel}">
        <view:StatusView />
    </DataTemplate>
</ResourceDictionary>